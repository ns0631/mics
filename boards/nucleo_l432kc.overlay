/ {
     continuous_servo: td8210mg {
          compatible = "pwm-servo";
          pwms = <&pwm1 2 PWM_MSEC(20) PWM_POLARITY_NORMAL>;
          min-pulse = <PWM_USEC(500)>;
          max-pulse = <PWM_USEC(2500)>;
     };

};

&timers1{
     pwm1: pwm {
          status = "okay";
          pinctrl-0 = <&tim1_ch2_pa9>;
          pinctrl-names = "default";
     };
};